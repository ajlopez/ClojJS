(ns tests.loop)(println "Loop tests")(defn recur?    [value]    (.isRecur js/recurs (value)))    (is (recur? (recur 1 2)))(let [result (recur 1 2 3)]    (is (= (.length result ()) 3))    (is (= (.get result (0)) 1))    (is (= (.get result (1)) 2))    (is (= (.get result (2)) 3)))(let [result (loop [x 0 y 3]            (if (> y 0)                (recur (+ x y) (- y 1))                x))]    (is (= result 6)))